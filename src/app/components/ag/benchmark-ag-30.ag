<script lang="ts">
  import { signal } from '@angular/core';

  const count30 = signal(0);

  function increment30() {
    count30.update((count) => count + 1);
  }
</script>

<template>
  <div class="benchmark-component">
    <h3>Component 30</h3>
    <button type="button" (click)="increment30()">
      Count {{ count30() }}
    </button>
  </div>
</template>

<style>
  .benchmark-component {
    padding: 1rem;
    border: 1px solid #ccc;
    margin: 0.5rem;
    border-radius: 4px;
  }
</style>
