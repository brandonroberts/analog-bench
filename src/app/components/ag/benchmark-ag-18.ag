<script lang="ts">
  import { signal } from '@angular/core';

  const count18 = signal(0);

  function increment18() {
    count18.update((count) => count + 1);
  }
</script>

<template>
  <div class="benchmark-component">
    <h3>Component 18</h3>
    <button type="button" (click)="increment18()">
      Count {{ count18() }}
    </button>
  </div>
</template>

<style>
  .benchmark-component {
    padding: 1rem;
    border: 1px solid #ccc;
    margin: 0.5rem;
    border-radius: 4px;
  }
</style>
